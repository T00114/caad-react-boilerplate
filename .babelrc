{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "browsers": [
            ">0.25%",
            "not ie 11",
            "not op_mini all"
          ],
          "modules": false,
          "useBuiltIns": true
        }
      }
    ],
    "react"
  ],
  "ignore": [
    "node_modules"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-object-assign",
    "transform-runtime",
    "react-hot-loader/babel"
  ]
}
